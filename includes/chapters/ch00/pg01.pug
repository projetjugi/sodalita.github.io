span.fstFrame
  img(src="/img/chapters/ch00/pg01/00.gif" data-aos="none" class="fr_00")

- var n = 1
while n <= 13
  - var f = n++

  if f < 10 
    +frame("ch00", "pg01", "0" + f, "none", "fr_0" + f)
  else if f >= 10
    +frame("ch00", "pg01", f, "none", "fr_" + f)